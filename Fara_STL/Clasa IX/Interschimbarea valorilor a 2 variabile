///METODA PAHARELOR

#include <bits/stdc++.h>
using namespace std;

int a=5, b=10, aux;

int main()
{
    aux = a;
    a   = b;
    b   = aux;
    return 0;
}




///FARA A FOLOSI VARIABILA AUXILIARA

#include <bits/stdc++.h>
using namespace std;

int a=5, b=10;

int main()
{
    a = a+b;
    b = a-b;
    a = a-b;
    return 0;
}
