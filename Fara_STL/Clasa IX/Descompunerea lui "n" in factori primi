#include <bits/stdc++.h>
using namespace std;

int n, d, k;

int main()
{
    cin>>n;
    d = 2;
    while(d*d<=n)
    {
        k = 0;
        while(n%d==0)
        {
            k++;
            n/=d;
        }
        if(k)
            cout<<d<<'^'<<k<<'\n';
        d++;
    }
    if(n!=1)
        cout<<n<<' '<<1;
}
